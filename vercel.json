{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/styles.css",
      "dest": "/static/styles.css"
    },
    {
      "src": "/static/script.js",
      "dest": "/static/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "BACKEND_URL": "https://sales-agent-hg11.onrender.com/"
  }
}
